    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "routes.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1",
                "PYTHONPATH": "server",
                "JIRA_SYNC_ORG": "capstone2020teamb",
                "JIRA_SYNC_USERNAME": "",
                "JIRA_SYNC_PASSWORD": "",
                "JAMA_SYNC_ORG": "capstone2020",
                "JAMA_SYNC_USERNAME": "",
                "JAMA_SYNC_PASSWORD": "",
                "SYNC_INTERVAL": "300"
            },
            "args": [
                "run",
                "--no-reload"
            ],
            "jinja": true
        }
    ]
}